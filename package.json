{
  "name": "runtime-typesafety",
  "version": "1.0.0",
  "description": "",
  "main": "index.mjs",
  "type": "module",
  "exports": {
    "./common.js": {
      "default": "./index.cjs"
    },
    ".": {
      "import": "./index.mjs",
      "require": "./index.cjs"
    }
  },
  "scripts": {
    "test-es6": "NODE_OPTIONS='--experimental-vm-modules' jest index.test.mjs",
    "test-cs": "                                          jest index.test.cjs",
    "test-simple": "npm run make; npm run test-cs",
    "test": "npm run make; npm run test-cs; npm run test-es6",
    "make": "node ./make.js"
  },
  "files": [
    "README.md",
    "index.cjs",
    "index.mjs",
    "package.json"
  ],
  "jest": {
    "moduleFileExtensions": [
      "js",
      "mjs",
      "cjs",
      "jsx",
      "ts",
      "tsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/*.test.[cm]js"
    ],
    "verbose": true
  },
  "author": {
    "name": "Atsushi Oka",
    "url": "https://github.com/a-oka-z/runtime-typesafety"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/a-oka-z/runtime-typesafety"
  },
  "license": "ISC",
  "devDependencies": {
    "sqlmacro": "^0.1.0"
  },
  "dependencies": {
    "fold-args": "^1.0.1",
    "prevent-undefined": "^0.2.33",
    "vanilla-schema-validator": "^2.0.4"
  }
}
